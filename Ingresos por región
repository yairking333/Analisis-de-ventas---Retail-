# Analizar ingresos por regi贸n
ingresos_por_region = ventas_merged.groupby('region')['revenue'].sum().reset_index()

# Graficar ingresos por regi贸n
plt.figure(figsize=(10, 6))
sns.barplot(x='revenue', y='region', data=ingresos_por_region)
plt.title('Ingresos por Regi贸n')
plt.xlabel('Ingresos')
plt.ylabel('Regi贸n')
plt.show()
